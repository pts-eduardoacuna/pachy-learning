{
  "pipeline": {
    "name": "inference"
  },
  "transform": {
    "image": "eduardoacye/pachy-learning-infer",
    "cmd": [
      "/infer",
      "-input-model=/pfs/model",
      "-input-digits-csv=/pfs/digits-csv",
      "-output-inference=/pfs/out"
    ]
  },
  "parallelism_spec": {
    "constant": "1"
  },
  "input": {
    "cross": [
      {
        "atom": {
          "repo": "digits-csv",
          "glob": "/*"
        }
      },
      {
        "atom": {
          "repo": "model",
          "glob": "/"
        }
      }
    ]
  }
}
