{
  "pipeline": {
    "name": "model"
  },
  "transform": {
    "image": "eduardoacye/pachy-learning-train",
    "cmd": [
      "/train",
      "-input-training=/pfs/mnist-csv",
      "-output-model=/pfs/out"
    ]
  },
  "parallelism_spec": {
    "constant": "1"
  },
  "input": {
    "atom": {
      "repo": "training",
      "glob": "/"
    }
  }
}
